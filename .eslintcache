)\n\n    return (<div>\n        <div className=\"ui form\">\n            <div className=\"field\">\n            <label>Enter Search Term</label>\n            <input value={term} onChange={e => setTerm(e.target.value)} className=\"input\"/>\n            </div>\n        </div>\n\n        <div className=\"ui celled list\">\n            {renderedResults}\n        </div>\n    </div>)\n}\n\nexport default Search","/home/gaurav/widgets/src/components/Dropdown.js",[],"/home/gaurav/widgets/src/components/Translate.js",["40"],"//AIzaSyCHUCmpR7cT_yDFHC98CZJy2LTms-IwDlM\nimport React,{useState,useEffect} from 'react'\nimport Dropdown from './Dropdown'\nimport Convert from './Convert'\nconst Translate =()=>{\n\n        const options =[\n            {\n                label:'Africaans',\n                value:'af'\n            },\n            {\n                label:'Arabic',\n                value:'ar'\n            },\n            {\n                label:'Hindi',\n                value:'hi'\n            }\n        ]\n\n        const [language,setLanguage] =useState(options[0])\n        const [text,setText]=useState('')\n\n    return (\n\n        <div>\n            <div className=\"ui form\">\n                <div className=\"field\">\n                    <label>Enter Text</label>\n                    <input value={text} onChange={(e)=>setText(e.target.value)}/>\n                </div>\n            </div>\n            \n            <Dropdown   label=\"Select a Language\"\n                        options={options} \n                        selected={language}\n                        onSelectedChange={setLanguage}\n            />\n\n            <h3 className=\"ui header\">\n                Output\n            </h3>\n            <Convert text={text} language={language}/>\n        </div>\n    )\n}\n\n\nexport default Translate","/home/gaurav/widgets/src/components/Convert.js",[],"/home/gaurav/widgets/src/components/Route.js",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","rep